(data jazz.ide.data.project


(import (jazz.editor.jazz))


(form
  (<Project>                 name: jedi                  catalog?: #t catalog-extensions: :jazz include-extensions: :jazz documentation: {File :context "doc" "jedi.jml"}
    (<*>                                                 tag-reference: {File :context "jedi.project"})
    (<Project-Directory>                                 directory: {Directory :context "src"}
      (<Project-Directory>                               directory: {Directory :context "src" "jedi"}
        (<Project-Directory>                             directory: {Directory :context "src" "jedi" "application"}
          (<Jazz-File>       name: Jedi-Menubar          source: {File :context "src" "jedi" "application" "Jedi-Menubar.jazz"})
          (<Jazz-File>       name: Jedi-Status           source: {File :context "src" "jedi" "application" "Jedi-Status.jazz"})
