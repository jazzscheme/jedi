        (<Jazz-File>         name: _jedi                 source: {File :context "src" "jedi" "_jedi.jazz"})
        (<Jazz-File>         name: autoload              source: {File :context "src" "jedi" "autoload.jazz"})
        (<Jazz-File>         name: run                   source: {File :context "src" "jedi" "run.jazz"})
