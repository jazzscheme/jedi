          (<Jazz-File>       name: Tree-Showcase-Actions source: {File :context "src" "jedi" "showcase" "Tree-Showcase-Actions.jazz"})
          (<Jazz-File>       name: Tree-Showcase         source: {File :context "src" "jedi" "showcase" "Tree-Showcase.jazz"}))
        (<Project-Directory>                             directory: {Directory :context "src" "jedi" "smoker"}
